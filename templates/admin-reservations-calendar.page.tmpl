{{ template "admin" .}}
{{ define "page-title" }}
Reservations Calendar
{{ end }}
{{ define "content"}}
<div class="col-md-12">
  <div class="text-center">
    <h3>
      {{index .StringMap "this_month"}} {{index .StringMap "this_month_year"}}
    </h3>
    <div class="d-flex flex-row">
      <a href='/admin/reservations-calendar?y={{index .StringMap "last_month_year"}}&m={{index .StringMap "last_month"}}'
      class="btn btn-sm btn-outline-secondary mr-auto">&lt;&lt;</a>
      <a href='/admin/reservations-calendar?y={{index .StringMap "next_month_year"}}&m={{index .StringMap "next_month"}}'
      class="btn btn-sm btn-outline-secondary ml-auto">&gt;&gt;</a>
    </div>
  </div>

</div>
{{ end }}
